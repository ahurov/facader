{
    "name": "gzhegow/facader",
    "description": "Static facades generator",
    "type": "library",
    "authors": [
        {
            "name": "gzhegow",
            "email": "6562680@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        
        "nette/php-generator": "^3.4",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        
        "symfony/var-dumper": "^4.0",
        
        "gzhegow/support": "^1.0",
        "gzhegow/di": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Gzhegow\\Facader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gzhegow\\Facader\\Tests\\": "tests/src"
        }
    }
}
