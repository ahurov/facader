{
    "name": "gzhegow/facader",
    "description": "Static facades generator",
    "type": "library",
    "authors": [
        {
            "name": "gzhegow",
            "email": "6562680@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/6562680/support"
        },
        {
            "type": "vcs",
            "url": "https://github.com/6562680/reflection"
        },
        {
            "type": "vcs",
            "url": "https://github.com/6562680/di"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        
        "nette/php-generator": "^3.4",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        
        "symfony/var-dumper": "^4.0",
        
        "gzhegow/support": "^1.0",
        "gzhegow/di": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Gzhegow\\Facader\\": "src/gzhegow/Facader"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gzhegow\\Facader\\Tests\\": "tests/src"
        }
    }
}
